        "# Contoh 3: Kelas Persegi\n",
        "class Persegi:\n",
        "    def __init__(self, sisi):\n",
        "        self.sisi = sisi\n",
        "\n",
        "    def luas(self):\n",
        "        return self.sisi ** 2\n",
        "\n",
        "# Membuat objek dari kelas Persegi\n",
        "persegi1 = Persegi(4)\n",
        "print(f\"Luas persegi: {persegi1.luas()}\")"
      ]
    }
  ]
